<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="edu.nju.software.pojo">
    <class name="TaskAssign" table="task_assign">
        <id name="id" column="id">
            <generator class="increment" />
        </id>
        <property name="taskId" type="integer" column="task_id" />
        <property name="memberId" type="integer" column="member_id" />
        <property name="outEmployeeId" type="integer" column="out_employee_id" />
    </class>

</hibernate-mapping>