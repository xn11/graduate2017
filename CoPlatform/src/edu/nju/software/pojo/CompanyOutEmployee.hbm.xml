<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="edu.nju.software.pojo">
    <class name="CompanyOutEmployee" table="company_out_employee">
        <id name="id" column="id">
            <generator class="increment" />
        </id>
        <property name="companyId" type="integer" column="company_id" />
        <property name="outEmployeeId" type="integer" column="out_employee_id" />
    </class>

</hibernate-mapping>
